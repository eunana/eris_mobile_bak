	<script type="text/javascript" src="<c:url value='/js/egovframework/mbl/cmm/jquery.mobile-1.4.5.js'/>"></script>
	<script type="text/javascript" src="<c:url value='/js/egovframework/mbl/cmm/EgovMobile-1.4.5.js'/>"></script>
  
	<!-- 신규공통컴포넌트 import -->
	<link rel="stylesheet" href="<c:url value='/css/egovframework/mbl/mcomd/egovMcomd.css'/>"/>
	<script type="text/javascript" src="<c:url value='/js/egovframework/mbl/com/mpa/mpa.js'/>"></script>
</head>
	
<body>

	<!--리스트 페이지 -->
	
	<div id="ErisFakeCall" data-role="page" data-theme="d">
		<div data-role="header" data-position="inline" data-theme="z">
			<a href="<c:url value='/index.jsp'/>" data-ajax="false" data-icon="home" class="ui-btn-left">메인</a>
			<h1><img src="<c:url value='/images/egovframework/mbl/mcomd/h1_logo.png'/>"/></h1>
			<div class="ui-body-a mcomd-title"><h3>FakeCall</h3></div>
		</div>
		
			
		<div data-role="content">
			<form name="FakeCallForm" method="post">
				<table width="100%" border="0" cellpadding="1" cellspacing="1" class="table-register" summary="fake call 정보입력">
				  <tr> 
				    <th width="20%" height="23" class="required_text" nowrap >이름&nbsp;&nbsp;</th>
				    <td width="80%" nowrap>
				        <input type="text" id="name"/>
				    </td>
				  </tr>
				  <tr> 
				    <th width="20%" height="23" class="required_text" nowrap >전화번호&nbsp;&nbsp;</th>
				    <td width="80%" nowrap>
				    	<input type="text" />
				    </td>
				  </tr>
				  <tr> 
				    <th width="20%" height="23" class="required_text" nowrap >날짜&nbsp;&nbsp;</th>
				    <td width="80%" nowrap>
				    	<input type="text" />
				    </td>
				  </tr>
				  <tr> 
				    <th height="23" class="required_text" nowrap >시간&nbsp;&nbsp;</th>
				    <td width="80%" nowrap>
				        <input type="text" />
				    </td>
				  </tr>
				</table>
			</form>
		</div>
		
		
		<div data-role="footer" data-theme="z" data-position="fixed" class="egovBar">
